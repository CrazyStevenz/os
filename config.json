        "workspaces": {
          "dynamicWorkspaces": true,
          "maxWorkspaces": 1
        }
      },
      "hyprland": {
        "enable": true,
        "backlight": "amdgpu_bl0",
        "lock": {
          "secondsToLowerBrightness": 60,
          "cpuUsageThreshold": 60,
          "diskUsageThreshold": 10,
          "networkUsageThreshold": 1000000
