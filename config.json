{
  "icedos": {
    "applications": {
      "android-tools": true,
      "brave": true,
      "codium": {
        "enable": true,
        "extensions": [
          "codezombiech.gitignore",
          "dbaeumer.vscode-eslint",
          "donjayamanne.githistory",
          "eamodio.gitlens",
          "editorconfig.editorconfig",
          "esbenp.prettier-vscode",
          "formulahendry.auto-close-tag",
          "formulahendry.code-runner",
          "gruntfuggly.todo-tree",
          "jnoortheen.nix-ide",
          "markwylde.vscode-filesize",
          "mblode.twig-language-2",
          "ms-vscode.references-view",
          "nico-castell.linux-desktop-file",
          "paragdiwan.gitpatch",
          "pkief.material-icon-theme",
          "timonwong.shellcheck",
          "zhuangtongfa.material-theme",
          "ziyasal.vscode-open-in-github"
        ],
        "zoomLevel": 0
      },
      "kitty": {
        "hideDecorations": true
      },
      "librewolf": {
        "enable": true,
        "overrides": true,
        "privacy": true,
        "pwas": [
          "https://app.tuta.com",
          "https://develop.element.io",
          "https://discord.com/app",
          "https://dtekteam.slack.com/",
          "https://web.skype.com/"
        ]
      },
      "nvchad": true,
      "steam": {
        "beta": true,
        "downloadsWorkaround": true,
        "session": {
          "enable": false,
          "autoStart": {
            "enable": false,
            "desktopSession": "hyprland"
          }
        }
      },
      "sunshine": true,
      "emulators": {
        "switch": false,
        "wiiu": false
      }
    },
    "bootloader": {
      "animation": false,
      "grub": {
        "enable": false,
        "device": "/dev/sda"
      },
      "systemd-boot": {
        "enable": true,
        "mountPoint": "/boot"
      }
    },
    "desktop": {
      "autologin": {
        "enable": true,
        "user": "icedborn"
      },
      "gdm": {
        "enable": true,
        "autoSuspend": true
      },
      "gnome": {
        "enable": true,
        "extensions": {
          "arcmenu": false,
          "dashToPanel": false,
          "gsconnect": true
        },
        "clock": {
          "date": false,
          "weekday": false
        },
        "hotCorners": false,
        "powerButtonAction": "interactive",
        "startupItems": false,
        "titlebarLayout": "appmenu:close",
        "workspaces": {
          "dynamicWorkspaces": true,
          "maxWorkspaces": 1
        }
      },
      "hyprland": {
        "enable": true,
        "backlight": "amdgpu_bl0",
        "lock": {
          "secondsToLowerBrightness": 60,
          "cpuUsageThreshold": 60,
          "diskUsageThreshold": 10,
          "networkUsageThreshold": 1000000
        }
      }
    }
  }
}
